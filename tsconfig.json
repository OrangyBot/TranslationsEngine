{
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "skipLibCheck": true,
    "outDir": "OrangyTranslationsEngine",
    "strict": true,
    "baseUrl": ".",
    "declaration": false
  }
}
